<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/dell/ontologies/2025/9/untitled-ontology-6/"
     xml:base="http://www.semanticweb.org/dell/ontologies/2025/9/untitled-ontology-6/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    
    <!-- Extension de l'ontologie Transport avec Réservation et Avis -->
    
    <!-- Classe Réservation -->
    <owl:Class rdf:about="http://www.semanticweb.org/dell/ontologies/2025/9/untitled-ontology-6/Réservation">
        <rdfs:label xml:lang="fr">Réservation</rdfs:label>
        <rdfs:comment xml:lang="fr">Une réservation effectuée par un passager pour un trajet</rdfs:comment>
    </owl:Class>
    
    <!-- Classe Avis -->
    <owl:Class rdf:about="http://www.semanticweb.org/dell/ontologies/2025/9/untitled-ontology-6/Avis">
        <rdfs:label xml:lang="fr">Avis</rdfs:label>
        <rdfs:comment xml:lang="fr">Un avis donné par un passager sur un trajet ou conducteur</rdfs:comment>
    </owl:Class>
    
    <!-- Propriétés pour Réservation -->
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dell/ontologies/2025/9/untitled-ontology-6/effectuePar">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/dell/ontologies/2025/9/untitled-ontology-6/Réservation"/>
        <rdfs:label xml:lang="fr">effectuée par</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dell/ontologies/2025/9/untitled-ontology-6/concerne">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/dell/ontologies/2025/9/untitled-ontology-6/Réservation"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/dell/ontologies/2025/9/untitled-ontology-6/Trajet"/>
        <rdfs:label xml:lang="fr">concerne</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/dell/ontologies/2025/9/untitled-ontology-6/dateReservation">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/dell/ontologies/2025/9/untitled-ontology-6/Réservation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:label xml:lang="fr">date de réservation</rdfs:label>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/dell/ontologies/2025/9/untitled-ontology-6/nombrePlaces">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/dell/ontologies/2025/9/untitled-ontology-6/Réservation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:label xml:lang="fr">nombre de places</rdfs:label>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/dell/ontologies/2025/9/untitled-ontology-6/statut">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/dell/ontologies/2025/9/untitled-ontology-6/Réservation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label xml:lang="fr">statut</rdfs:label>
        <rdfs:comment xml:lang="fr">Statut de la réservation: En attente, Confirmée, Annulée</rdfs:comment>
    </owl:DatatypeProperty>
    
    <!-- Propriétés pour Avis -->
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dell/ontologies/2025/9/untitled-ontology-6/donnePar">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/dell/ontologies/2025/9/untitled-ontology-6/Avis"/>
        <rdfs:label xml:lang="fr">donné par</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dell/ontologies/2025/9/untitled-ontology-6/concerneTrajet">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/dell/ontologies/2025/9/untitled-ontology-6/Avis"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/dell/ontologies/2025/9/untitled-ontology-6/Trajet"/>
        <rdfs:label xml:lang="fr">concerne trajet</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dell/ontologies/2025/9/untitled-ontology-6/concerneConducteur">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/dell/ontologies/2025/9/untitled-ontology-6/Avis"/>
        <rdfs:label xml:lang="fr">concerne conducteur</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/dell/ontologies/2025/9/untitled-ontology-6/note">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/dell/ontologies/2025/9/untitled-ontology-6/Avis"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:label xml:lang="fr">note</rdfs:label>
        <rdfs:comment xml:lang="fr">Note de 1 à 5 étoiles</rdfs:comment>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/dell/ontologies/2025/9/untitled-ontology-6/commentaire">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/dell/ontologies/2025/9/untitled-ontology-6/Avis"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label xml:lang="fr">commentaire</rdfs:label>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/dell/ontologies/2025/9/untitled-ontology-6/dateAvis">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/dell/ontologies/2025/9/untitled-ontology-6/Avis"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:label xml:lang="fr">date de l'avis</rdfs:label>
    </owl:DatatypeProperty>
    
</rdf:RDF>
